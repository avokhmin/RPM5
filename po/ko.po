msgstr "이 프로그램은 GNU GPL 하에서 자유롭게 재 배포될 수 있습니다"

#: rpm.c:248
msgid "Usage: rpm {--help}"
msgstr "사용법: rpm {--help}"

#: rpm.c:249
msgid "       rpm {--version}"
msgstr "        rpm {--version}"

#: rpm.c:250
msgid "       rpm {--initdb}   [--dbpath <dir>]"
msgstr "        rpm {--initdb} [--dbpath <디렉토리>]"

#: rpm.c:251
msgid ""
"       rpm {--install -i} [-v] [--hash -h] [--percent] [--force] [--test]"
msgstr ""
"        rpm {--install -i} [-v] [--hash -h] [--percent] [--force] [--test]"

#: rpm.c:252
msgid "                        [--replacepkgs] [--replacefiles] [--root <dir>]"
msgstr ""
"                     [--replacepkgs] [--replacefiles] [--root <디렉토리>]"

#: rpm.c:253
msgid "                        [--excludedocs] [--includedocs] [--noscripts]"
msgstr "                     [--excludedocs] [--includedocs] [--noscripts]"

#: rpm.c:254
msgid ""
"                        [--rcfile <file>] [--ignorearch] [--dbpath <dir>]"
msgstr ""
"                     [--rcfile <파일>] [--ignorearch] [--dbpath <디렉토리>]"

#: rpm.c:255
msgid ""
"                        [--prefix <dir>] [--ignoreos] [--nodeps] [--allfiles]"
msgstr ""
"                     [--prefix <디렉토리>] [--ignoreos] [--nodeps] "
"[--allfiles]"

#: rpm.c:256 rpm.c:265 rpm.c:275
msgid "                        [--ftpproxy <host>] [--ftpport <port>]"
msgstr "                     [--ftpproxy <호스트>] [--ftpport <포트>]"

#: rpm.c:257 rpm.c:276
msgid "                        [--httpproxy <host>] [--httpport <port>]"
msgstr "                     [--httpproxy <호스트>] [--httpport <포트>]"

#: rpm.c:258
msgid ""
"                        [--justdb] [--noorder] [--relocate oldpath=newpath]"
msgstr ""
"                     [--justdb] [--noorder] [--relocate 이전경로=새로운경로]"

#: rpm.c:259
msgid ""
"                        [--badreloc] [--notriggers] [--excludepath <path>]"
msgstr ""
"                     [--badreloc] [--notriggers] [--excludepath <경로>]"

#: rpm.c:260
msgid "                        [--ignoresize] file1.rpm ... fileN.rpm"
msgstr "                     [--ignoresize] 파일1.rpm .. 파일N.rpm"

#: rpm.c:261
msgid ""
"       rpm {--upgrade -U} [-v] [--hash -h] [--percent] [--force] [--test]"
msgstr ""
"        rpm {--upgrade -U} [-v] [--hash -h] [--percent] [--force] [--test]"

#: rpm.c:262
msgid "                        [--oldpackage] [--root <dir>] [--noscripts]"
msgstr "                     [--oldpackage] [--root <디렉토리>] [--noscripts]"

#: rpm.c:263
msgid ""
"                        [--excludedocs] [--includedocs] [--rcfile <file>]"
msgstr "                     [--excludedocs] [--includedocs] [--rcfile <파일>]"

#: rpm.c:264
msgid ""
"                        [--ignorearch]  [--dbpath <dir>] [--prefix <dir>] "
msgstr ""
"                     [--ignorearch]  [--dbpath <디렉토리>] [--prefix "
"<디렉토리>]"

#: rpm.c:266
msgid "                        [--httpproxy <host>] [--httpport <port>] "
msgstr "                     [--httpproxy <호스트>] [--httpport <포트>]"

#: rpm.c:267
msgid "                        [--ignoreos] [--nodeps] [--allfiles] [--justdb]"
msgstr "                     [--ignoreos] [--nodeps] [--allfiles] [--justdb]"

#: rpm.c:268
msgid "                        [--noorder] [--relocate oldpath=newpath]"
msgstr "                     [--noorder] [--relocate 이전경로=새로운경로]"

#: rpm.c:269
msgid ""
"                        [--badreloc] [--excludepath <path>] [--ignoresize]"
msgstr ""
"                     [--badreloc] [--excludepath <경로>] [--ignoresize]"

#: rpm.c:270
msgid "                        file1.rpm ... fileN.rpm"
msgstr "                     파일1.rpm .. 파일N.rpm"

#: rpm.c:271
msgid "       rpm {--query -q} [-afpg] [-i] [-l] [-s] [-d] [-c] [-v] [-R]"
msgstr "        rpm {--query -q} [-afpg] [-i] [-l] [-s] [-d] [-c] [-v] [-R]"

#: rpm.c:272
msgid "                        [--scripts] [--root <dir>] [--rcfile <file>]"
msgstr "                     [--scripts] [--root <디렉토리>] [--rcfile <파일>]"

#: rpm.c:273
msgid "                        [--whatprovides] [--whatrequires] [--requires]"
msgstr "                     [--whatprovides] [--whatrequires] [--requires]"

#: rpm.c:274
msgid "                        [--triggeredby]"
msgstr "                     [--triggeredby]"

#: rpm.c:277
msgid "                        [--provides] [--triggers] [--dump]"
msgstr "                     [--provides] [--triggers] [--dump]"

#: rpm.c:278
msgid "                        [--changelog] [--dbpath <dir>] [targets]"
msgstr "                     [--changelog] [--dbpath <디렉토리>] [목표대상]"

#: rpm.c:279
msgid "       rpm {--verify -V -y} [-afpg] [--root <dir>] [--rcfile <file>]"
msgstr ""
"        rpm {--verify -V -y} [-afpg] [--root <디렉토리>] [--rcfile <파일>]"

#: rpm.c:280
msgid ""
"                        [--dbpath <dir>] [--nodeps] [--nofiles] [--noscripts]"
msgstr ""
"                     [--dbpath <디렉토리>] [--nodeps] [--nofiles] "
"[--noscripts]"

#: rpm.c:281
msgid "                        [--nomd5] [targets]"
msgstr "                     [--nomd5] [목표대상]"

#: rpm.c:282
msgid "       rpm {--setperms} [-afpg] [target]"
msgstr "        rpm {--setperms} [-afpg] [목표대상]"

#: rpm.c:283
msgid "       rpm {--setugids} [-afpg] [target]"
msgstr "        rpm {--setugids} [-afpg] [목표대상]"

#: rpm.c:284
msgid "       rpm {--freshen -F} file1.rpm ... fileN.rpm"
msgstr "        rpm {--freshen -F} 파일1.rpm .. 파일N.rpm"

#: rpm.c:285
msgid "       rpm {--erase -e} [--root <dir>] [--noscripts] [--rcfile <file>]"
msgstr ""
"        rpm {--erase -e} [--root <디렉토리>] [--noscripts] [--rcfile <파일>]"

#: rpm.c:286
msgid "                        [--dbpath <dir>] [--nodeps] [--allmatches]"
msgstr ""
"                         [--dbpath <디렉토리>] [--nodeps] [--allmatches]"

#: rpm.c:287
msgid "                        [--justdb] [--notriggers] package1 ... packageN"
msgstr "                         [--justdb] [--notriggers] 패키지1 .. 패키지N"

#: rpm.c:288
msgid "       rpm {--resign} [--rcfile <file>] package1 package2 ... packageN"
msgstr "        rpm {--resign}   [--rcfile <파일>] 패키지1 패키지2 .. 패키지N"

#: rpm.c:289
msgid "       rpm {--addsign} [--rcfile <file>] package1 package2 ... packageN"
msgstr "        rpm {--addsign}  [--rcfile <파일>] 패키지1 패키지2 .. 패키지N"

#: rpm.c:290
msgid ""
"       rpm {--checksig -K} [--nopgp] [--nogpg] [--nomd5] [--rcfile <file>]"
msgstr ""
"        rpm {--checksig -K} [--nopgp] [--nogpg] [--nomd5] [--rcfile <파일>]"

#: rpm.c:291
msgid "                           package1 ... packageN"
msgstr "                            패키지1 .. 패키지N"

#: rpm.c:292
msgid "       rpm {--rebuilddb} [--rcfile <file>] [--dbpath <dir>]"
msgstr "        rpm {--rebuilddb} [--rcfile <파일>] [--dbpath <디렉토리>]"

#: rpm.c:293
msgid "       rpm {--querytags}"
msgstr "        rpm {--querytags}"

#: rpm.c:327 rpmqv.c:479
msgid "Usage:"
msgstr "  사용법:"

#: rpm.c:329 rpmqv.c:481
msgid "print this message"
msgstr "이 메세지를 출력합니다"

#: rpm.c:331 rpmqv.c:130 rpmqv.c:483
msgid "print the version of rpm being used"
msgstr "현재 사용 중인 rpm 버전을 표시합니다"

#: rpm.c:334
msgid "   All modes support the following arguments:"
msgstr "   모든 모드에서 다음의 인수를 사용하실 수 있습니다:"

#: rpm.c:335
msgid "    --define '<name> <body>'"
msgstr "    --define '<이름> <본문>'"

#: rpm.c:336 rpmqv.c:137 rpmqv.c:488
msgid "define macro <name> with value <body>"
msgstr "<본문> 의 내용을 <이름> 매크로로 정의합니다"

#: rpm.c:337
msgid "    --eval '<name>+'      "
msgstr "    --eval '<이름>+'      "

#: rpm.c:338
msgid "print the expansion of macro <name> to stdout"
msgstr "<이름> 매크로의 확장을 표준출력으로 출력합니다"

#: rpm.c:339
msgid "    --pipe <cmd>          "
msgstr "    --pipe <명령>         "

#: rpm.c:340 rpmqv.c:143 rpmqv.c:492
msgid "send stdout to <cmd>"
msgstr "표준출력을 <명령> 으로 보냅니다"

#: rpm.c:341
msgid "    --rcfile <file>       "
msgstr "    --rcfile <파일>       "

#: rpm.c:342
msgid "use <file> instead of /etc/rpmrc and $HOME/.rpmrc"
msgstr "/etc/rpmrc 와 $HOME/.rpmrc 파일 대신에 <파일> 을 사용합니다"

#: rpm.c:344 rpmqv.c:161 rpmqv.c:496
msgid "display final rpmrc and macro configuration"
msgstr "현재 설정되어 있는 rpmrc 의 내용과 매크로를 보여줍니다"

#: rpm.c:346 rpmqv.c:504
msgid "be a little more verbose"
msgstr "화면에 출력되는 메세지의 내용을 자세히 보여줍니다"

#: rpm.c:348 rpmqv.c:506
msgid "be incredibly verbose (for debugging)"
msgstr "화면에 출력되는 메세지의 내용을 아주 상세히 보여줍니다 (디버깅 용)"

#: rpm.c:351
msgid "   Install, upgrade and query (with -p) allow URL's to be used in place"
msgstr ""
"     설치, 업그레이드 및 질의시 (-p 옵션과 함께 사용시) 파일명을 적는 곳에"

#: rpm.c:352
msgid "   of file names as well as the following options:"
msgstr "      파일이 위치한 URL 과 함께 다음의 옵션을 사용하실 수 있습니다:"

#: rpm.c:353
msgid "      --ftpproxy <host>   "
msgstr "      --ftpproxy <호스트> "

#: rpm.c:354 rpmqv.c:513
msgid "hostname or IP of ftp proxy"
msgstr "ftp 프록시의 호스트 명 또는 IP"

#: rpm.c:355
msgid "      --ftpport <port>    "
msgstr "      --ftpport <포트>    "

#: rpm.c:356 rpmqv.c:515
msgid "port number of ftp server (or proxy)"
msgstr "ftp 서버 (또는 프록시) 의 포트 번호"

#: rpm.c:357
msgid "      --httpproxy <host>  "
msgstr "      --httpproxy <호스트>"

#: rpm.c:358 rpmqv.c:517
msgid "hostname or IP of http proxy"
msgstr "http 프록시의 호스트 명 또는 IP"

#: rpm.c:359
msgid "      --httpport <port>   "
msgstr "      --httpport <포트>   "

#: rpm.c:360 rpmqv.c:519
msgid "port number of http server (or proxy)"
msgstr "http 서버 (또는 프록시) 의 포트 번호"

#: rpm.c:364 rpmqv.c:539
msgid "query mode"
msgstr "질의 모드"

#: rpm.c:365 rpm.c:411 rpm.c:436 rpm.c:488 rpm.c:560
msgid "      --dbpath <dir>      "
msgstr "      --dbpath <디렉토리> "

#: rpm.c:366 rpm.c:412 rpm.c:437 rpm.c:489 rpm.c:561 rpmqv.c:499
msgid "use <dir> as the directory for the database"
msgstr "<디렉토리> 를 데이터베이스 디렉토리로 사용합니다"

#: rpm.c:367
msgid "      --queryformat <qfmt>"
msgstr "      --queryformat <질의형식>"

#: rpm.c:368 rpmqv.c:541
msgid "use <qfmt> as the header format (implies --info)"
msgstr "<질의형식> 을 헤더 형식으로 사용합니다 (--info 옵션이 포함되어 있음)"

#: rpm.c:369 rpm.c:413 rpm.c:471 rpm.c:500
msgid "      --root <dir>        "
msgstr "      --root <디렉토리>   "

#: rpm.c:370 rpm.c:414 rpm.c:472 rpm.c:501 rpm.c:563 rpmqv.c:146 rpmqv.c:501
msgid "use <dir> as the top level directory"
msgstr "<디렉토리> 를 최상위 디렉토리로 사용합니다"

#: rpm.c:371
msgid "      Package specification options:"
msgstr "       패키지 지정 옵션:"

#: rpm.c:373
msgid "query all packages"
msgstr "모든 패키지에 대해 질의합니다"

#: rpm.c:374
msgid "        -f <file>+        "
msgstr "        -f <파일>+        "

#: rpm.c:375
msgid "query package owning <file>"
msgstr "<파일> 이 포함되어 있는 패키지에 대해 질의합니다"

#: rpm.c:376
msgid "        -p <packagefile>+ "
msgstr "        -p <패키지파일>+  "

#: rpm.c:377
msgid "query (uninstalled) package <packagefile>"
msgstr "(설치되지 않은) <패키지파일> 에 대해 질의합니다"

#: rpm.c:378
msgid "        --triggeredby <pkg>"
msgstr "        --triggeredby <패키지>"

#: rpm.c:379
msgid "query packages triggered by <pkg>"
msgstr "<패키지> 로 인하여 발생되는 패키지에 대해 질의합니다"

#: rpm.c:380
msgid "        --whatprovides <cap>"
msgstr "        --whatprovides <기능>"

#: rpm.c:381
msgid "query packages which provide <cap> capability"
msgstr "<기능> 을 제공하는 패키지에 대해 질의합니다"

#: rpm.c:382
msgid "        --whatrequires <cap>"
msgstr "        --whatrequires <기능>"

#: rpm.c:383
msgid "query packages which require <cap> capability"
msgstr "<기능> 을 필요로 하는 패키지에 대해 질의합니다"

#: rpm.c:384
msgid "      Information selection options:"
msgstr "       정보 선택 옵션:"

#: rpm.c:386 rpmqv.c:545
msgid "display package information"
msgstr "패키지에 대한 정보를 보여줍니다"

#: rpm.c:388 rpmqv.c:547
msgid "display the package's change log"
msgstr "패키지의 변경 사항(change log)을 보여줍니다"

#: rpm.c:390 rpmqv.c:549
msgid "display package file list"
msgstr "패키지 안에 포함된 파일의 목록을 보여줍니다"

#: rpm.c:392 rpmqv.c:551
msgid "show file states (implies -l)"
msgstr "패키지 안에 포함된 파일의 상태를 보여줍니다 (-l 옵션이 포함되어 있음)"

#: rpm.c:394 rpmqv.c:553
msgid "list only documentation files (implies -l)"
msgstr ""
"패키지 안에 포함된 파일 중 문서 파일만 보여줍니다 (-l 옵션이 포함되어 있음)"

#: rpm.c:396 rpmqv.c:555
msgid "list only configuration files (implies -l)"
msgstr ""
"패키지 안에 포함된 파일 중 설정 파일만 보여줍니다 (-l 옵션이 포함되어 있음)"

#: rpm.c:398 rpmqv.c:557
msgid ""
"show all verifiable information for each file (must be used with -l, -c, or "
"-d)"
msgstr ""
"패키지 안의 각 파일에 대해 검증할 수 있는 정보는 모두 보여줍니다 (-l, -c, "
"또는 -d 옵션과 함께 사용해야 합니다)"

#: rpm.c:400
msgid "list capabilities package provides"
msgstr "패키지가 제공하는 기능을 나열합니다"

#: rpm.c:402
msgid "list package dependencies"
msgstr "패키지와 의존성이 있는 패키지를 나열합니다"

#: rpm.c:404
msgid "print the various [un]install scripts"
msgstr "[제거]설치시에 사용되는 스크립트를 출력합니다"

#: rpm.c:406
msgid "show the trigger scripts contained in the package"
msgstr "패키지에 포함된 트리거 스크립트를 보여줍니다"

#: rpm.c:410 rpmqv.c:568
msgid ""
"verify a package installation using the same same package specification "
"options as -q"
msgstr ""
"-q 옵션과 같은 패키지 지정 옵션을 사용하여 설치되어 있는 패키지를 검사합니다"

#: lib/poptBT.c:187 lib/verify.c:60 rpm.c:416 rpm.c:458 rpm.c:493 rpmqv.c:270
#: rpmqv.c:570 rpmqv.c:618 rpmqv.c:652
msgid "do not verify package dependencies"
msgstr "패키지의 의존성을 검사하지 않습니다"

#: lib/verify.c:66 rpm.c:418 rpmqv.c:210 rpmqv.c:574
msgid "do not verify file md5 checksums"
msgstr "파일의 md5 체크섬을 검사하지 않습니다"

#: rpm.c:420 rpmqv.c:572
msgid "do not verify file attributes"
msgstr "파일의 속성을 검사하지 않습니다"

#: rpm.c:422 rpmqv.c:579
msgid "list the tags that can be used in a query format"
msgstr "질의 형식에서 사용될 수 있는 태그를 나열합니다"

#: rpm.c:425
msgid "    --install <packagefile>"
msgstr "    --install <패키지파일>"

#: rpm.c:426
msgid "    -i <packagefile>      "
msgstr "    -i <패키지파일>       "

#: rpm.c:427 rpmqv.c:266 rpmqv.c:593
msgid "install package"
msgstr "패키지를 설치합니다"

#: rpm.c:428
msgid "      --excludepath <path>"
msgstr "      --excludepath <경로>"

#: rpm.c:429
msgid "skip files in path <path>"
msgstr "<경로> 안의 파일은 설치하지 않습니다"

#: rpm.c:430
msgid "      --relocate <oldpath>=<newpath>"
msgstr "      --relocate <이전경로>=<새로운경로>"

#: rpm.c:431 rpmqv.c:630
msgid "relocate files from <oldpath> to <newpath>"
msgstr "<이전경로> 에서 <새로운경로> 로 파일을 재 배치하여 설치합니다"

