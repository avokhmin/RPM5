#: rpm.c:286 rpmqv.c:396
msgid "Usage:"
msgstr "Использование:"

#: rpm.c:288 rpmqv.c:398
msgid "print this message"
msgstr "вывести это сообщение"

#: rpm.c:290 rpmqv.c:400
msgid "print the version of rpm being used"
msgstr "вывести версию используемого rpm"

#: rpm.c:293
msgid "   All modes support the following arguments:"
msgstr "   Все режимы поддерживают следующие аргументы:"

#: rpm.c:294
msgid "    --define '<name> <body>'"
msgstr "    --define '<имя> <подстановка>'"

